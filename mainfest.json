{
  "manifest_version": 2,
  "name": "Outcognito",
  "version": "2.0",
  "description": "Outcognito is priceless! Ask no more, you can @Call_",
  "permissions": [
    "webRequestBlocking",
    "webRequest",
    "activeTab",
    "tabs",
    "http://*.facebook.com/*",
    "https://*.facebook.com/*",
    "https://*.youtube.com/",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
